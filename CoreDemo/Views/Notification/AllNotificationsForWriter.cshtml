@model List<EntityLayer.Concrete.Notification>

@{
    ViewData["Title"] = "AllNotificationsForWriter";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}

<h1>Bildirimler</h1>
<table class="table-bordered">
    <tr>
        <th>#</th>
        <th>Bildirim</th>
        <th>Tarig</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.NotificationID</td>
            <td>@item.NotificationType</td>
            <td>@item.NotificationDate</td>
        </tr>
    }
</table>

